<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{100} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name ="com.yang.generator.mapper" level="TRACE"/>
    <root level="ERROR">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>